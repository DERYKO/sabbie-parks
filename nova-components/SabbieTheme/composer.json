{
    "name": "deryko/sabbie-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Deryko\\SabbieTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deryko\\SabbieTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
