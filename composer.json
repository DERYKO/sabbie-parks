{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "africastalking/africastalking": "^3.0",
        "php": "^7.1.3",
        "ext-curl": "*",
        "brozot/laravel-fcm": "^1.3",
        "deryko/mapping-select": "*",
        "deryko/mappings": "*",
        "deryko/sabbie-theme": "*",
        "dingo/api": "^2.4",
        "example/miami-ice": "*",
        "fideloper/proxy": "^4.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "inspheric/nova-url-field": "^1.4",
        "laravel/framework": "5.7.*",
        "laravel/nova": "*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/Mappings"
        },
        {
            "type": "path",
            "url": "./nova-components/Mappings"
        },
        {
            "type": "path",
            "url": "./nova-components/MappingSelect"
        },
        {
            "type": "path",
            "url": "./nova-components/MappingSelect"
        },
        {
            "type": "path",
            "url": "./nova-components/MiamiIce"
        },
        {
            "type": "path",
            "url": "./nova-components/SabbieTheme"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
